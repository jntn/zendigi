{
    "version": 2,
    "name": "zendigi-api",
    "builds": [
        {
            "src": "index.go",
            "use": "@now/go",
            "config": {
                "maxLambdaSize": "10mb"
            }
        }
    ],
    "routes": [
        {
            "src": "/query",
            "dest": "/index.go"
        }
    ]
}